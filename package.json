{
  "name": "rango-client",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=20",
    "npm": "please-use-yarn"
  },
  "workspaces": {
    "packages": [
      "examples/*",
      "widget/*",
      "queue-manager/*",
      "wallets/*",
      "signers/*",
      "logging/*"
    ],
    "nohoist": [
      "**/@types/web"
    ]
  },
  "scripts": {
    "clean-root": "rimraf .parcel-cache && nx clear-cache",
    "clean": "yarn clean-root && nx run-many --target=clean",
    "prepare": "husky install && yarn i18n:compile",
    "build:logging": "nx run-many --target=build  --projects=@arlert-dev/logging-*",
    "build:signers": "nx run-many --target=build --projects=@arlert-dev/signer-* --parallel=5",
    "build:wallets": "nx run-many --target=build --projects=@arlert-dev/signer-*,@arlert-dev/wallets-*,@arlert-dev/provider-* --parallel=5",
    "build:queue-manager": "nx run-many --target=build --projects=@arlert-dev/queue-manager-* --exclude=@arlert-dev/queue-manager-demo",
    "build:widget": "nx run-many --target=build  --projects=@arlert-dev/widget-*",
    "build:iframe": "nx run-many --target=build  --projects=@arlert-dev/widget-iframe",
    "build:all": "nx run-many --target=build",
    "build": "nx affected --target=build --parallel=3",
    "build:non-demo": "nx affected --target=build --parallel=3 --exclude=@arlert-dev/queue-manager-demo,@arlert-dev/widget-app,@arlert-dev/widget-playground",
    "dev:ui": "nx run @arlert-dev/storybook:dev",
    "dev:queue-manager": "node ./scripts/dev-watch/command.mjs --project @arlert-dev/queue-manager-demo",
    "dev:widget:type-checker": "node ./scripts/dev-ts-check/command.mjs --project @arlert-dev/widget-app",
    "dev:widget:dev-server": "yarn --cwd ./widget/app/ dev",
    "dev:widget": "npm-run-all --parallel dev:widget:dev-server dev:widget:type-checker",
    "dev:playground:type-checker": "node ./scripts/dev-ts-check/command.mjs --project @arlert-dev/widget-playground",
    "dev:playground:dev-server": "yarn --cwd ./widget/playground/ dev",
    "dev:playground": "npm-run-all --parallel dev:playground:dev-server dev:playground:type-checker",
    "lint": "nx run-many --target=lint",
    "format": "nx run-many --target=format",
    "publish": "node ./scripts/publish/command.mjs",
    "deploy": "node ./scripts/deploy/command.mjs",
    "release-prod": "node ./scripts/release/command.mjs",
    "post-release-prod": "node ./scripts/post-release/command.mjs",
    "upgrade-all": "node ./scripts/upgrade-all/command.mjs --project ",
    "i18n:extract": "lingui extract --clean",
    "i18n:compile": "lingui compile --typescript",
    "i18n": "yarn i18n:extract && yarn i18n:compile",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "commitlint": "commitlint --edit",
    "postinstall": "patch-package && yarn workspace @arlert-dev/signer-cosmos run patch-package"
  },
  "dependencies": {
    "husky": "^8.0.3",
    "tslib": "^2.4.1",
    "typescript": "^5.7.3"
  },
  "devDependencies": {
    "@actions/core": "^1.10.1",
    "@babel/core": "^7.22.5",
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@conventional-changelog/git-client": "^2.5.1",
    "@eslint/compat": "^1.2.5",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.19.0",
    "@faker-js/faker": "^8.4.1",
    "@lingui/cli": "^4.2.1",
    "@nrwl/workspace": "^15.9.2",
    "@testing-library/dom": "^10.1.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@vercel/routing-utils": "^3.1.0",
    "@vitest/coverage-v8": "^2.0.5",
    "buffer": "^5.5.0",
    "chalk": "^5.2.0",
    "command-line-args": "^5.2.1",
    "conventional-changelog": "^7.1.0",
    "conventional-changelog-angular": "^8.0.0",
    "conventional-changelog-cli": "^2.2.2",
    "conventional-commits-filter": "^5.0.0",
    "conventional-commits-parser": "^6.2.0",
    "conventional-recommended-bump": "^6.1.0",
    "crypto-browserify": "^3.12.0",
    "esbuild": "^0.20.1",
    "esbuild-plugins-node-modules-polyfill": "^1.6.6",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-rango": "^0.0.1",
    "eslint-plugin-destructuring": "^2.2.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-id-attribute-enforcement": "^1.0.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "execa": "^7.1.1",
    "filesize": "^10.1.2",
    "globals": "^15.14.0",
    "happy-dom": "^14.12.0",
    "lint-staged": "^13.2.2",
    "npm-run-all": "^4.1.5",
    "nx": "16.2.1",
    "os-browserify": "^0.3.0",
    "parcel": "^2.11.0",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.7.1",
    "punycode": "^1.4.1",
    "querystring-es3": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.3.1",
    "rimraf": "^5.0.1",
    "semver-parser": "^4.1.4",
    "stream-browserify": "^3.0.0",
    "tty-browserify": "^0.0.1",
    "typescript-eslint": "^8.21.0",
    "util": "^0.12.3",
    "vitest": "^2.0.5",
    "vm-browserify": "^1.1.2"
  },
  "resolutions": {
    "valibot": "^0.38.0",
    "react-refresh": "0.9.0",
    "protobufjs": "^6.11.4",
    "@types/node": "^20.17.19"
  },
  "@parcel/resolver-default": {
    "packageExports": true
  },
  "packageManager": "yarn@1.22.19+sha1.4ba7fc5c6e704fce2066ecbfb0b0d8976fe62447"
}
