{
  "name": "@arlert-dev/widget-app",
  "version": "0.39.0",
  "license": "MIT",
  "private": true,
  "source": "public/index.html",
  "main": "dist/index.html",
  "targets": {
    "main": false
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "dev": "parcel -p 3002 --cache-dir=.parcel-cache --no-cache",
    "build": "parcel build --cache-dir=.parcel-cache",
    "ts-check": "tsc --declaration --emitDeclarationOnly -p ./tsconfig.json",
    "clean": "rimraf .parcel-cache && rimraf dist",
    "format": "prettier --write '{.,src}/**/*.{ts,tsx}'",
    "lint": "eslint \"**/*.{ts,tsx}\""
  },
  "devDependencies": {},
  "dependencies": {
    "@arlert-dev/logging-console": "^0.9.1-next.1",
    "@arlert-dev/logging-subscriber": "^0.9.1-next.1",
    "@arlert-dev/widget-embedded": "^0.46.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0"
  }
}
