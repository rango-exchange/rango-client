    "rango-sdk": "^0.1.23",
    "rango-types": "^0.1.28",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.2.0",
    "react-router-dom": "^6.8.0",
    "zustand": "^4.3.2"
