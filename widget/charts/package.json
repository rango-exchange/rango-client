    "lint": "eslint \"**/*.{ts,tsx}\" --ignore-path ../../.eslintignore"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "dependencies": {
    "@visx/axis": "2.18.0",
    "@visx/event": "2.17.0",
    "@visx/shape": "2.18.0",
    "@visx/grid": "2.18.0",
    "@visx/tooltip": "2.17.0",
    "@visx/scale": "2.18.0",
    "dayjs": "^1.11.6",
    "@rango-dev/ui": "^0.37.1-next.2"
  },
  "publishConfig": {
    "access": "public"
  }
