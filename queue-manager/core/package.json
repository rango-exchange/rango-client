{
  "name": "@rango-dev/queue-manager-core",
  "version": "0.22.1-next.0",
  "license": "MIT",
  "type": "module",
  "source": "./src/index.ts",
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
